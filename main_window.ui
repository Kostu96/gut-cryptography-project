<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Toplevel" id="top_level">
    <property name="geometry">640x480</property>
    <property name="height">480</property>
    <property name="takefocus">false</property>
    <property name="width">640</property>
    <child>
      <object class="ttk.Frame" id="main_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Notebook" id="notebook">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="expand">true</property>
              <property name="fill">both</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="ttk.Notebook.Tab" id="key_generation_tab">
                <property name="text" translatable="yes">Generowanie kluczy</property>
                <child>
                  <object class="ttk.Frame" id="frame_4">
                    <property name="height">200</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="ttk.Label" id="label_2">
                        <property name="text" translatable="yes">Wybierz algorytm</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="row">0</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Combobox" id="generateComboBox">
                        <property name="state">readonly</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="row">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Entry" id="private_key_entry">
                        <property name="state">readonly</property>
                        <property name="textvariable">string:private_key</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="ipadx">5</property>
                          <property name="padx">0</property>
                          <property name="pady">5</property>
                          <property name="row">3</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Entry" id="public_key_entry">
                        <property name="state">readonly</property>
                        <property name="textvariable">string:public_key</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="ipadx">5</property>
                          <property name="pady">5</property>
                          <property name="row">4</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Label" id="label_5">
                        <property name="text" translatable="yes">Klucz prywatny</property>
                        <layout manager="grid">
                          <property name="column">1</property>
                          <property name="padx">5</property>
                          <property name="row">3</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Label" id="label_6">
                        <property name="text" translatable="yes">Klucz publiczny</property>
                        <layout manager="grid">
                          <property name="column">1</property>
                          <property name="row">4</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Frame" id="frame_1">
                        <property name="height">100</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="row">5</property>
                          <property name="sticky">w</property>
                        </layout>
                        <child>
                          <object class="ttk.Button" id="button_3">
                            <property name="command" type="command" cbtype="simple">generateKeys</property>
                            <property name="text" translatable="yes">Generuj klucze</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">0</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Button" id="button_4">
                            <property name="command" type="command" cbtype="simple">saveKeys</property>
                            <property name="text" translatable="yes">Zapisz klucze</property>
                            <layout manager="grid">
                              <property name="column">1</property>
                              <property name="padx">5</property>
                              <property name="row">0</property>
                            </layout>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Combobox" id="keyLengthComboBox">
                        <property name="state">readonly</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="row">2</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Notebook.Tab" id="encryption_tab">
                <property name="text" translatable="yes">Szyfrowanie</property>
                <child>
                  <object class="ttk.Frame" id="frame_5">
                    <property name="height">200</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Notebook.Tab" id="decryption_tab">
                <property name="text" translatable="yes">Deszyfrowanie</property>
                <child>
                  <object class="ttk.Frame" id="frame_6">
                    <property name="height">200</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Menu" id="menu6">
    <child>
      <object class="tk.Menuitem.Submenu" id="menu_file">
        <property name="label" translatable="yes">File</property>
        <child>
          <object class="tk.Menuitem.Command" id="menu_exit">
            <property name="command" type="command" cbtype="simple">exit</property>
            <property name="label" translatable="yes">Exit</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Menuitem.Submenu" id="menu_help">
        <property name="label" translatable="yes">Help</property>
      </object>
    </child>
  </object>
</interface>
